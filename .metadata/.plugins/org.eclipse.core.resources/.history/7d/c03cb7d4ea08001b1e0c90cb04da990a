package hoy.app;

public class HtmlConverterApp {

	public static void main(String[] args) {
		String html = "<h1>Grocery List</h1>\n" +
	              "<ul>\n" +
	              "    <li>Eggs</li>\n" +
	              "    <li>Milk</li>\n" +
	              "    <li>Butter</li>\n" +
	              "</ul>";

		System.out.println("HTML Input:\n" + html);
		
		html = html.replace("<h1>", "");
		
		System.out.println("Text Output:\n" + html);

	}

}
